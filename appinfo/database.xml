<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	 <name>*dbname*</name>
	 <create>true</create>
	 <overwrite>false</overwrite>
	 <charset>utf8</charset>
	 <table>
		<name>*dbprefix*internal_bookmarks</name>
		<declaration>
			<field>
				<name>bkid</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<unsigned>true</unsigned>
				<length>4</length>
			</field>
			<field>
				<name>uid</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>bktitle</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>128</length>
			</field>
			<field>
				<name>bktarget</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>255</length>
			</field>
			<field>
				<name>bkorder</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>4</length>
			</field>
			<index>
				<name>bk_pk</name>
			    <primary>true</primary>
			    <field>
					<name>bkid</name>
					<sorting>ascending</sorting>
				</field>
			</index>
		</declaration>
	</table>
</database>
